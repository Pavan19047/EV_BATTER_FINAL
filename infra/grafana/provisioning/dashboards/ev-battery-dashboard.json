          },
          {
            "datasource": {
              "type": "postgres",
              "uid": "PCC52D03280B7034C"
            },
            "rawSql": "SELECT ts as time, battery_temperature as \"Temperature (\u00b0C)\" FROM telemetry WHERE $__timeFilter(ts) ORDER BY ts",
            "refId": "B",
            "format": "time_series"
          }
        ],
        "fieldConfig": {
          "defaults": {}
        }
      },
      {
